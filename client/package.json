{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/react-hooks": "^3.1.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "classnames": "^2.2.6",
    "cross-fetch": "^3.0.5",
    "graphql": "^15.3.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "init:graphql": "node src/scripts/fetchPossibleTypes",
    "watch:css": "postcss src/styles/tailwind.css -o src/generated/styles/main.css -w",
    "build:css": "postcss src/styles/tailwind.css -o src/generated/styles/main.css",
    "prestart": "npm run init:graphql",
    "prebuild": "NODE_ENV=production npm run build:css && npm run init:graphql",
    "start": "npm run watch:css & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.4.6"
  }
}
